{
	"name": "meta3d",
	"private": true,
	"scripts": {
		"clean": "lerna clean",
		"bootstrap": "lerna bootstrap",
		"build": "lerna run build:ts",
		"publish:all": "npm config set registry https://registry.npmjs.org/ && lerna run publish:self && lerna publish",
		"meta3d:publish_dev": "lerna run meta3d:publish_dev --concurrency 1",
		"meta3d:publish_pro": "lerna run meta3d:publish_pro --concurrency 1",
		"ci:test": "lerna run test:ci --concurrency 1"
	},
	"workspaces": [
		"protocols/extension_protocols/*",
		"protocols/contribute_protocols/*",
		"extensions/*",
		"contributes/*",
		"defaults/*",
		"examples/*",
		"platform/*",
		"services/*",
		"utils/*"
	],
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	},
	"devDependencies": {
		"lerna": "^4.0.0"
	},
	"resolutions": {
		"jest-environment-jsdom": "^26.0.1",
		"jest": "^26.4.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	}
}