{
	"name": "meta3d",
	"private": true,
	"scripts": {
		"clean": "lerna clean",
		"install": "lerna bootstrap",
		"run:editor": "lerna run buildAll:ci --concurrency 1 && cd examples/use-editor && yarn webpack:dev-server ",
		"fastRun:editor": "cd examples/use-editor && yarn webpack:dev-server ",
		"run:engine": "cd examples/use-engine && yarn webpack:dev-server ",
		"run:app": "cd examples/app && yarn webpack:dev-server ",
		"build": "lerna run build:ts",
		"ci:test": "lerna run test:ci --concurrency 1"
	},
	"workspaces": [
		"examples/*",
		"contribute/",
		"defaults/*",
		"extensions/*",
		"extension_protocols/*",
		"contribute/test_contribute_protocols/*",
		"contribute/test_contributes/*",
		"engine/*"
	],
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	},
	"devDependencies": {
		"lerna": "^4.0.0"
	},
	"resolutions": {
		"jest-environment-jsdom": "^26.0.1"
	}
}