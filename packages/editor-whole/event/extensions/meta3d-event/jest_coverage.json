{
    "automock": false,
    "moduleFileExtensions": [
        "js"
    ],
    "testRegex": "lib/js/test/.+/.+_test\\.bs\\.js$",
    "transformIgnorePatterns": [
        ""
    ],
    "setupFilesAfterEnv": [
        "<rootDir>/test/helper/jest/specHelper.js"
    ],
    "notify": false,
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage/",
    "coveragePathIgnorePatterns": [
        "<rootDir>/lib/js/src/Main.*",
        "<rootDir>/lib/js/src/EventManager.*",
        "<rootDir>/lib/js/src/.+/api/.*",
        "<rootDir>/lib/js/src/.+/HandleMouseEventDoService.+"
    ],
    "collectCoverageFrom": [
        "lib/js/src/**/*.js",
        "!**/node_modules/**",
        "!**/vendor/**"
    ],
    "coverageThreshold": {
        "global": {
            "lines": 90,
            "statements": 90,
            "branches": 50,
            "functions": 89
        }
    }
}