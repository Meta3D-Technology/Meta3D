        "meta3d-type": "^1.0.1"
    },
    "devDependencies": {
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "cz-customizable": "^6.3.0",
        "gulp": "^4.0.2",
        "meta3d-tool-publish": "^1.0.1",
        "source-map-loader": "^3.0.0",
        "ts-loader": "^9.2.6",
