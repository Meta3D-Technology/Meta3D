    "babel-jest": "^26.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.2.0",
    "cz-customizable": "^6.3.0",
    "gentype": "^3.48.0",
    "gulp": "^4.0.2",
    "jest": "^25.2.3",
    "meta3d-tool-publish": "^0.21.0",
    "webpack": "^5.62.1",
