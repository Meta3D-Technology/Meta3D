{
    "name": "meta3d-engine-core",
    "version": "0.4.1",
    "publisher": "meta3d",
    "protocol": {
        "name": "meta3d-engine-core-protocol"
    },
    "dependentExtensionNameMap": {
        "meta3dBsMostExtensionName": {
            "protocolName": "meta3d-bs-most-protocol"
        }
    },
    "dependentContributeNameMap": {},
    "scripts": {
        "watch": "rescript clean && rescript build -w",
        "buildAll:ci": "rescript clean && rescript build",
        "test:watch": "jest --watch --config jest.json",
        "test:coverage": "jest --maxWorkers=4 --config jest_coverage.json",
        "test:ci": "yarn buildAll:ci && jest --maxWorkers=4 --config jest_coverage.json",
        "webpack": "webpack --config webpack.config.js",
        "meta3d:publish_dev": "yarn buildAll:ci && NODE_ENV=development npm run webpack && gulp publish",
        "meta3d:publish_pro": "yarn buildAll:ci && NODE_ENV=production npm run webpack && gulp publish"
    },
    "dependencies": {
        "meta3d-bs-most-protocol": "^0.4.1",
        "meta3d-engine-core-protocol": "^0.4.1",
        "meta3d-type": "^0.4.1",
        "most": "1.7.0",
        "most-subject": "^5.3.0",
        "rescript": "^9.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "clean-webpack-plugin": "^4.0.0",
        "cz-customizable": "^6.3.0",
        "gentype": "^3.48.0",
        "gulp": "^4.0.2",
        "meta3d-tool-publish": "^0.4.1",
        "webpack": "^5.62.1",
        "webpack-cli": "^4.9.1"
    }
}
