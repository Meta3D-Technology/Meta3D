{
    "name": "meta3d-webgl1",
    "version": "0.5.1",
    "publisher": "meta3d",
    "protocol": {
        "name": "meta3d-webgl1-protocol"
    },
    "dependentExtensionNameMap": {},
    "dependentContributeNameMap": {},
    "scripts": {
        "watch": "rescript clean && rescript build -w",
        "buildAll:ci": "rescript clean && rescript build",
        "webpack": "webpack --config webpack.config.js",
        "meta3d:publish_dev": "yarn buildAll:ci && NODE_ENV=development npm run webpack && gulp publish",
        "meta3d:publish_pro": "yarn buildAll:ci && NODE_ENV=production npm run webpack && gulp publish"
    },
    "dependencies": {
        "meta3d-webgl1-protocol": "^0.5.1",
        "rescript": "^9.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "clean-webpack-plugin": "^4.0.0",
        "cz-customizable": "^6.3.0",
        "gentype": "^3.48.0",
        "gulp": "^4.0.2",
        "meta3d-tool-publish": "^0.5.0",
        "webpack": "^5.62.1",
        "webpack-cli": "^4.9.1"
    }
}
