{
    "name": "meta3d-use-editor",
    "version": "0.5.0",
    "publisher": "meta3d",
    "protocol": {
        "name": "meta3d-use-editor-protocol"
    },
    "dependentExtensionNameMap": {
        "meta3dEventExtensionName": {
            "protocolName": "meta3d-event-protocol"
        },
        "meta3dUIExtensionName": {
            "protocolName": "meta3d-ui-protocol"
        },
        "meta3dImguiRendererExtensionName": {
            "protocolName": "meta3d-imgui-renderer-protocol"
        },
        "meta3dBindIOEventExtensionName": {
            "protocolName": "meta3d-bind-io-event-protocol"
        }
    },
    "dependentContributeNameMap": {
        "meta3dSkinDefaultContributeName": {
            "protocolName": "meta3d-skin-default-protocol"
        },
        "meta3dCustomControlButtonContributeName": {
            "protocolName": "meta3d-custom-control-button-protocol"
        },
        "meta3dElementButtonContributeName": {
            "protocolName": "meta3d-element-button-protocol"
        },
        "meta3dElementButton2ContributeName": {
            "protocolName": "meta3d-element-button2-protocol"
        },
        "meta3dEventClickButtonContributeName": {
            "protocolName": "meta3d-event-click-button-protocol"
        }
    },
    "scripts": {
        "watch": "tsc -w",
        "webpack": "webpack --config webpack.config.js",
        "meta3d:publish_dev": "NODE_ENV=development npm run webpack && gulp publish",
        "meta3d:publish_pro": "NODE_ENV=production npm run webpack && gulp publish"
    },
    "dependencies": {
        "meta3d-bind-io-event-protocol": "^0.5.0",
        "meta3d-custom-control-button-protocol": "^0.5.0",
        "meta3d-element-button-protocol": "^0.5.0",
        "meta3d-element-button2-protocol": "^0.5.0",
        "meta3d-event-click-button-protocol": "^0.5.0",
        "meta3d-event-protocol": "^0.5.0",
        "meta3d-imgui-renderer-protocol": "^0.5.0",
        "meta3d-skin-default-protocol": "^0.5.0",
        "meta3d-type": "^0.5.0",
        "meta3d-ui-protocol": "^0.5.0",
        "meta3d-use-editor-protocol": "^0.5.0"
    },
    "devDependencies": {
        "clean-webpack-plugin": "^4.0.0",
        "cz-customizable": "^6.3.0",
        "gulp": "^4.0.2",
        "meta3d-tool-publish": "^0.5.0",
        "source-map-loader": "^3.0.0",
        "ts-loader": "^9.2.6",
        "typescript": "^4.2.3",
        "webpack": "^5.62.1",
        "webpack-cli": "^4.9.1"
    }
}
