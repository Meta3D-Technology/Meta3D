{
    "name": "frontend",
    "version": "1.0.0-beta.4",
    "authors": "Wonder",
    "scripts": {
        "bundle_dts": "gulp bundle_dts",
        "watch": "gulp changeToLocalEnv &&  rescript clean && rescript build -w",
        "watch_pro": "gulp changeToProductionEnv &&  rescript clean && rescript build -w",
        "buildAll:ci": "rescript clean && rescript build",
        "buildAll:dev": "gulp changeToLocalEnv && rescript clean && rescript build && gulp restoreEnv",
        "buildAll:pro": "gulp changeToProductionEnv && rescript clean && rescript build && gulp restoreEnv",
        "webpack_dev": "yarn buildAll:dev && cross-env NODE_ENV=development webpack --config webpack.config.js",
        "webpack_pro": "yarn buildAll:pro && cross-env NODE_ENV=production webpack --config webpack.config.js",
        "webpack:dev-server": "cross-env NODE_ENV=development webpack-dev-server  --config webpack.config.devserver.js",
        "test:watch": "jest --maxWorkers=4 --watch --config jest.json",
        "test:update": "jest --maxWorkers=4 -u --config jest.json",
        "test:coverage": "jest --maxWorkers=4 --config jest_coverage.json",
        "test:ci": "yarn buildAll:ci && jest --maxWorkers=4 --config jest_coverage.json --updateSnapshot"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@rescript/react": "0.10.3",
        "babel-jest": "^26.1.0",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^3.6.0",
        "cz-customizable": "^5.2.0",
        "dts-bundle-generator": "^9.0.0",
        "gentype": "^3.48.0",
        "html-webpack-plugin": "^5.5.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.4.2",
        "jest-cucumber": "^3.0.1",
        "mini-css-extract-plugin": "^0.9.0",
        "monaco-editor-webpack-plugin": "^7.1.0",
        "react-test-renderer": "^16.8.3",
        "style-loader": "^1.2.1",
        "webpack": "^5.62.1",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.4.0",
        "write-file-webpack-plugin": "^4.5.1"
    },
    "dependencies": {
        "@ant-design/graphs": "^1.4.1",
        "@cloudbase/js-sdk": "^1.7.2",
        "antd": "^5.12.2",
        "fbemitter": "^3.0.0",
        "localforage": "^1.10.0",
        "meta3d-bs-most-default": "^1.0.0-beta.4",
        "meta3d-editor-whole-protocol": "^1.0.0-beta.4",
        "meta3d-react-monaco-editor": "^1.0.0-beta.4",
        "moment": "^2.29.4",
        "remporium": "^0.0.1",
        "rescript": "^10.0.1",
        "rescript-indexeddb": "^0.0.3"
    }
}
