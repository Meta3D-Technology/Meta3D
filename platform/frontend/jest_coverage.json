{
    "automock": false,
    "moduleFileExtensions": [
        "js"
    ],
    "testRegex": "lib/es6_global/test/step-definitions/.+.steps.bs.js",
    "transformIgnorePatterns": [
        "<rootDir>/node_modules/jest-cucumber"
    ],
    "moduleNameMapper": {
        "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "snapshotResolver": "<rootDir>/customSnapshotResolver.js",
    "setupFilesAfterEnv": [
        "<rootDir>/test/helper/jest/specHelper.js"
    ],
    "notify": false,
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage/",
    "coveragePathIgnorePatterns": [
        "<rootDir>/lib/es6_global/src/.+/AssembleSpace.*",
        "<rootDir>/lib/es6_global/src/.+/ApAssemble.*",
        "<rootDir>/lib/es6_global/src/.+/ElementAssemble.*",
        "<rootDir>/lib/es6_global/src/.+/ExtensionInspector.*",
        "<rootDir>/lib/es6_global/src/.+/ContributeInspector.*",
        "<rootDir>/lib/es6_global/src/.+/PackageInspector.*",
        "<rootDir>/lib/es6_global/src/.+/ApDependencyGraph.*",
        "<rootDir>/lib/es6_global/src/.+/PackageDependencyGraph.*",
        "<rootDir>/lib/es6_global/src/.+/Most.*",
        "<rootDir>/lib/es6_global/src/.+/.*Store.*"
    ],
    "collectCoverageFrom": [
        "lib/es6_global/src/external_layer/ui/app/assemble_space/**/*.js",
        "!**/node_modules/**",
        "!**/vendor/**"
    ],
    "coverageThreshold": {
        "global": {
            "lines": 60,
            "statements": 60,
            "branches": 49,
            "functions": 49
        }
    }
}