{
  "name": "meta3d-bs-jest",
  "version": "0.12.0",
  "description": "BuckleScript bindings to the Jest testing framework for meta3d",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "test": "npm run build && jest",
    "watch:bsb": "bsb -make-world -w",
    "watch:jest": "jest --watchAll",
    "watch:screen": "screen -c .screenrc"
  },
  "keywords": [
    "meta3d",
    "jest",
    "bucklescript",
    "test"
  ],
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "4.0.18"
  },
  "dependencies": {
    "jest": "24.3.1"
  }
}
